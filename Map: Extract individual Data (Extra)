const movies = [
    {
        title: 'Star Wars',
        rating: 100
    },
    {
        title: 'Hitman',
        rating: 99
    },
    {
        title: 'Alien',
        rating: 98
    }
]

const titles = movies.map(function(movie){
    return movie.title; //returns the titles only
})

