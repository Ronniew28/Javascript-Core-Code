const numbers = [1, 2, 3, 4, 5, 6, 7];

numbers.filter(n => {
    return n === 4
})
//--------------------------------------------
const numbers1 = [1, 2, 3, 4, 5, 6, 7];

numbers1.filter(z => {
    return z <= 5
})
