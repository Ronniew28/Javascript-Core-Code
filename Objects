//------------------------------------------------------------------------------------------

const sample  =  {
    dogs: 7,
    cats: 5,
    horses: 3,
    mice: 2
}
console.log(sample);
//property is 2 parts.
//key and value
// eg; dogs & 7
console.log(sample.horses); //3

const name = {firstName: 'Tom' , secondName: 'Jones'};
console.log(name);

const random = {
    number: 5,
    words: 'Hello World',
    tags: ['#one', '#two', '#three'],
    isOk: true
};
console.log(random);
