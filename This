//------------------------------------------------------------------------------
const cat = {
    name: 'Tiddles',
    color: 'Tabby',
    breed: 'Mixed',
    //add a method
    meow() {
        //console.log("Purr Purr"); //standard output
        console.log(this.color);
        console.log(`${this.name} purrs`);
    }
}
//---------------------------------------------------------
const dog = {
    name: 'Fido',
    color: 'Black & White',
    breed: 'Sheepdog',
    woof() {
        console.log(this.name);
        console.log(`${this.breed}`);
    }
}
