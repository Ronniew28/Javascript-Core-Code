const comments = [ //NB!!!: use []
    {username: 'Tammy', 
    remark: 'hello',
    votes: 7} ,

    {username: 'Tom' ,
    remark: 'hi',
    votes: 9},

    {username: 'Dick' ,
    remark: 'welcome' ,
    votes: 5}
]
    console.log(comments);
    console.log(comments[1]); //outputs the 'Tom' array contents
    console.log(comments[1].remark); //ouput 'hi
    
