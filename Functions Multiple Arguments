function greet (firstName, lastName) {
    console.log(`Hello ${firstName} ${lastName}`);
}
greet("Tom", "Jones");
//---------------------------------------------------------

function title (nameOne, nameTwo, nameThree) {
    console.log(`Names are ${nameOne} ${nameTwo} ${nameThree}`);
}
greet("Tom" , "Dick" , "Harry");
//----------------------------------------------------------
