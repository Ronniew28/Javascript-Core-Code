//--------------------------------------------------------------
const numbers = [1, 2, 3, 4, 5, 6, 7];

    function print(element) {
        console.log(element);
    }
    numbers.forEach(print) //outputs numbers 1 to 7
    
    numbers.forEach(function (el) {
        console.log(el) //output numbers 1 to 7
    })

    for (let el of numbers) {
        console.log(el); //output numbers 1 to 7
    }
