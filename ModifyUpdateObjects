//--------------------------------------------------------------------------------------------

const midterms = {Dani: 98, Tom: 75};
console.log(midterms);

midterms.Tom = 79;
console.log(midterms);
//Toms score has been updated

midterms.Dani = 'A+';
midterms.Tom = 'B';
console.log(midterms);

midterms.Mike = 'F';
console.log(midterms);

midterms['Jess'] = 'C';
console.log(midterms);
//Note the use of [] which is also valid
