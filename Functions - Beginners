function sing() {
    console.log("La La");
    console.log("Squeak Squeak");
    console.log("Me, Me, Me");
}
sing() //for the function to run, it mmust be called

//--------------------------------------------------------

function alpha() {
    console.log("alpha");
    console.log("beta");
    console.log("gamma");
}
alpha()
//--------------------------------------------------------

function printHeart() {
    console.log("<3");
    console.log("<3");
    console.log("<3");
}
printHeart()
