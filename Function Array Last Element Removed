function  lastElement() {
    return [1, 2, 3];
}
let [num1, num2] = lastElement();

console.log(num1);
console.log(num2);
//Array contents in the function are output
//NB:This is array destructuring
//--------------------------------------------------------------
var last =  function(array, n) {
    if (array == null) 
      return void 0;
    if (n == null) 
       return array[array.length - 1];
    return array.slice(Math.max(array.length - n, 0));  
    };
    console.log(last([7, 9, 0, -2]));
//-----------------------------------------------------------------

