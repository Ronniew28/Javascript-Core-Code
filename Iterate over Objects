//--------------------------------------------------------------------------------
const exampleObj = {
    Tom: 80,
    Mike: 65,
    Andy: 47,
    Jack: 55,
    Ian: 10,
    Colin: 99
}

for (let key in exampleObj) {
    if (exampleObj.hasOwnProperty(key)) {
        value = exampleObj[key];
        console.log(key, value);
    }
}
