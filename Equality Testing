//-----------------------------------------------------------------------------

let num1 = [1, 2, 3];
let num2 = [1, 2, 3];
console.log(num1 === num2);
//returns false as the array contents are not compared
//returns false as memory locations are whats compared

//-----------------------------------------------
let test = [4, 5, 6]
let testCopy = test;
console.log(test);
console.log(testCopy);
test.push(7);
console.log(test);
console.log(testCopy); //output [4, 5, 6, 7] as its a copt of the test array
//------------------------------------------------
console.log(test === testCopy);
//true as the same memory location is used
