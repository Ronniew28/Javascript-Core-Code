const movies = [
    {
        title: 'Star Wars',
        score: 100,
        year: 1977
    } ,
    {
        title: 'Amadeus',
        score: 85,
        year: 1984
    },
    {
        title: 'The Philosophers Stone',
        score: 95,
        year: 2005
    }
]

const name = movies.filter(m => m.score > 90)
