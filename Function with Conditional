function snakeEyes (num1, num2) {
    //console.log(`${num1} ${num2} Not Snake Eyes`);
    if (snakeEyes == 1, 1) {
        console.log(`${num1} ${num2} Snake Eyes`);
    } else if
        (snakeEyes != 1, 1)
        console.log(`${num1} ${num2} Not Snake Eyes`);

}
snakeEyes(1, 1);
