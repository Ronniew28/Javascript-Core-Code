//----------------------------------------------------------------------

const numbers = [ 77,53, 19, 98,2]

//Adding the array contents without using reduce
/* total = 0;
    for (let price of numbers) {
        total += price;
    }
console.log(total)    
*/

//----------------------------------------------

//Adding the array contents using reduce
const total = numbers.reduce((total, price)  => {
    return total + price; //249
})

//Using an implicit return
const total1 = numbers.reduce((total, price) => total + price)
