//--------------------------------------------------------------------
function callTwice(func) {
    func();
    func();
}
//------------------------------------------------------------

function rollDie() {
    const roll = Math.floor(Math.random() * 7 +1);
    console.log(roll);
}

callTwice(rollDie);

